""""""""""""""""""""""""""""""""""""""""
" Settings file for GUI Vim (MacVim)
""""""""""""""""""""""""""""""""""""""""
set guifont=Iosevka:h18
"set quifont=Inconsolata:h20
set linespace=4

"colorscheme solarized
"set background=light  "use with solarized colorscheme
colorscheme gruvbox

" Below is for session management
:let g:session_autoload = 'no'
:let g:session_autosave = 'no'


set nocompatible
if has("autocmd")
  filetype plugin indent on
endif


